{
  "name": "mrbs-tablet",
  "version": "0.2.0",
  "scripts": {
    "setup": "`which bash` -c 'cd app && npm install && cd public && bower install'",
    "build": "`which bash` -c 'cd gulp && gulp'",
    "watch": "`which bash` -c 'cd gulp && gulp watch'",
    "push": "`which bash` -c 'docker build -t docker-repo.lowecloud.com/mrbs-tablet:0.3 . && docker push docker-repo.lowecloud.com/mrbs-tablet:0.3'"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-sass": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0"
  },
  "dependencies": {
    "autoprefixer": "^6.1.2",
    "gulp-postcss": "^6.0.1"
  }
}
